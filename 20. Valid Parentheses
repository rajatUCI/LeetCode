class Solution {
public:
    bool isValid(string s) {
        int a[6] = {0};
        for(int i = 0; i < s.size(); i++){
            if(s[i] == '(')
                a[0]++;
            else if(s[i] == ')')
                a[1]++;
            else if(s[i] == '{')
                a[2]++;
            else if(s[i] == '}')
                a[3]++;
            else if(s[i] == '[')
                a[4]++;
            else if(s[i] == ']')
                a[5]++;
        }
        if(a[0] == a[1] && a[2] == a[3] && a[4] == a[5])
            return true;
        return false;
    }
};